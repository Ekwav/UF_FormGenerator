{% macro generate(name, value) %}
    <div class="checkbox icheck">
        <input type="checkbox" name="{{value.name}}" id="{{value.id}}" class="{% if value.class %}{{value.class}}{% endif %}" {% if value.data == 1 %}checked{% endif %} {% if value.disabled %}disabled{% endif %}></input>
    </div>
{% endmacro %}